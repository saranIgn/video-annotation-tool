{
    "compilerOptions": {
      "target": "ES6",
      "module": "ESNext",
      "declaration": true,
      "declarationDir": "./dist",
      "emitDeclarationOnly": false,
      "outDir": "./dist",
      "jsx": "react-jsx",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true
    },
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
  }
  